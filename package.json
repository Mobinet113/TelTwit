  "license": "GNU General Public License",
  "engines": {
    "npm": ">=5.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mobinet113/TelTwit-Announcer"
  },

  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
